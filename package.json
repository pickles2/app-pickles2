        "cheerio": "^0.22.0",
        "cmd-queue": "github:tomk79/node-cmd-queue#develop",
        "csv": "^2.0.0",
        "ejs": "^3.1.7",
        "express": "^4.17.3",
        "express-pickles2": "github:tomk79/node-express-pickles2#develop",
        "express-queue": "0.0.12",
        "file-stream-rotator": "0.0.7",
