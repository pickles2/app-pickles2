        "gulp-browserify": "^0.5.1",
        "gulp-concat": "^2.6.1",
        "gulp-plumber": "^1.1.0",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "^5.1.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^5.5.0",
        "laravel-mix": "^6.0.48",
        "nw": "0.79.0-sdk",
