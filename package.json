    "extra": {
        "network": {
            "preview": {
                "port": 58080
            },
            "appserver": {
                "port": 58081
            }
        },
        "dataDirName": ".pickles2desktoptool",
        "colors": {
            "defaultKeyColor": "#00a0e6"
        },
        "forum": {
            "url": "https://github.com/pickles2/app-pickles2/issues",
            "title": "GitHub issue"
        },
        "credit": "(C)Pickles Project."
    },
    "manifestUrl": "https://pickles2.pxt.jp/release/app/Pickles2/package.json",
    "devManifestUrl": "https://pickles2.pxt.jp/release/app_dev/Pickles2/package.json",
    "window": {
        "title": "Pickles 2",
        "toolbar": false,
        "frame": true,
        "width": 340,
        "height": 420,
        "fullscreen": false,
        "icon": "app/common/images/appicon.png",
        "position": "center"
    },
    "platformOverrides": {
        "win": {
            "window": {
                "toolbar": false
            }
        },
        "osx": {
            "window": {
                "toolbar": false
            }
        },
        "linux": {
            "window": {
                "toolbar": false
            }
        }
